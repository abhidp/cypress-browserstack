{
  "compilerOptions": {
    "target": "ES6",
    "types": ["cypress", "mocha"],
    "module": "commonjs",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "strict": true,
    "lib": ["es6", "dom"],
    "baseUrl": ".",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true,
    "checkJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["./**/*.ts", "node_modules/cypress"]
}
